{
  "name": "skedr.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "VUE_APP_RELEASE_VERSION=$(sentry-cli releases propose-version) vue-cli-service build",
    "postbuild": "./sentryRelease.sh",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:watch": "vue-cli-service test:unit --watch --coverage"
  },
  "dependencies": {
    "@sentry/browser": "^4.6.4",
    "apollo-cache-inmemory": "^1.2.2",
    "apollo-client": "^2.3.2",
    "apollo-link": "^1.2.2",
    "apollo-link-context": "^1.0.9",
    "apollo-link-http": "^1.5.4",
    "aws-amplify": "^1.0.0",
    "graphql": "^14.0.0",
    "graphql-tag": "^2.9.2",
    "greenkeeper-lockfile": "^1.15.1",
    "intersection-observer": "^0.5.0",
    "lodash": "^4.17.10",
    "logrocket": "^0.6.17",
    "logrocket-vuex": "^0.0.3",
    "moment": "^2.22.1",
    "vue": "^2.6.8",
    "vue-analytics": "^5.12.2",
    "vue-apollo": "^3.0.0-beta.16",
    "vue-router": "^3.0.1",
    "vuetify": "^1.4.0",
    "vuetify-loader": "^1.0.5",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "@sentry/cli": "^1.40.0",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-plugin-unit-jest": "^3.1.1",
    "@vue/cli-service": "^3.1.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "codecov": "^3.0.2",
    "lint-staged": "^7.2.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-vuetify": "^0.2.0",
    "vue-template-compiler": "^2.5.16",
    "webpack-bundle-analyzer": "^3.0.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "frontend": "Skedr.io APP",
  "gitHooks": {
    "pre-commit": "lint-staged && yarn test:unit"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
